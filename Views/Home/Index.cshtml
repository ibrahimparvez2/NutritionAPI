@model IEnumerable<CountingKsDataLayer.Pocos.Food>
@{
    ViewBag.Title = "Home Page";
}
<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">Nutrition API Starting Page.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>
<div class="row">
    @section featured {
        <section class="featured">
            <div class="content-wrapper">
            </div>
        </section>
    }
    <h3>Some sample results from the database:</h3>
    @foreach (var food in Model)
    {
        <div>@food.Description</div>
        <ul>
            @foreach (var measure in food.Measures)
            {
                <li>@measure.Description: @measure.Calories calories</li>
            }
        </ul>
    }
</div>
